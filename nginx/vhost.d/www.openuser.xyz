location /home/ {
    root /home/realestkma/app/dist/;
    index index.html;
    try_files $uri $uri/ /index.html;
}

location /static/ {
    alias /home/realestkma/app/staticfiles/;
    add_header Access-Control-Allow-Origin *;
}

location /media/ {
    alias /home/realestkma/app/mediafiles/;
    add_header Access-Control-Allow-Origin *;
}